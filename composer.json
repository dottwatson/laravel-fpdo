{
    "name": "dottwatson/laravel-fpdo",
    "description": "A MySQL engine written in PHP for interct wwith Flat Files",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Mirko Temperini",
            "email": "mirko.temperini@gmail.com"
        }
    ],
    "require": {
        "php": "^7.1|^8",
        "ext-pdo": "*",
        "dottwatson/php-mysql-engine":"dev-master"
    },
    "repositories":[ 
        { 
            "type": "vcs", 
            "url": "git@github.com:dottwatson/php-mysql-engine" 
        } 
    ],
    "autoload": {
        "psr-4": {
            "Fpdo\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Fpdo\\FpdoServiceProvider"
            ]
        }
    }
}